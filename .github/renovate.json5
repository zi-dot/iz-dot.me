{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchPaths": ["package.json"],
      "matchDepTypes": ["dependencies"],
      "groupName": "dependencies"
    },
    {
      "matchPaths": ["package.json"],
      "matchDepTypes": ["devDependencies"],
      "groupName": "devDependencies"
    }
  ],
  "ignoreDeps": [
    "@react-three/fiber"
  ]
}
